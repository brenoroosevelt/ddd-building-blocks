{
    "name": "brenoroosevelt/ddd-building-blocks",
    "description": "DDD Building Blocks",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "ramsey/uuid": "*",
        "brenoroosevelt/php-specifications": "*",
        "brenoroosevelt/oni-bus": "@stable",
        "brenoroosevelt/php-attributes": "@stable",
        "brenoroosevelt/habemus": "@stable"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.6"
    },
    "autoload": {
        "psr-4": {
            "BrenoRoosevelt\\DDD\\BuildingBlocks\\" : "src/",
            "BrenoRoosevelt\\DDD\\BuildingBlocks\\Test\\" : "tests/"
        }
    },
    "minimum-stability": "dev"
}
